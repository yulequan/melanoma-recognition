layer {
  name: "score-5c"
  type: "Convolution"
  bottom: "res5c"
  top: "score-5c"
  param {
    lr_mult: 0.1
    decay_mult: 1
  }
  param {
    lr_mult: 0.2
    decay_mult: 0
  }
  convolution_param {
    num_output: 2
    kernel_size: 1
    engine: CUDNN
  }
}


layer {
  name: "upscore-5c"
  type: "Deconvolution"
  bottom: "score-5c"
  top: "upscore-5c"
  convolution_param {
    kernel_size: 4
    stride: 2
    num_output: 2
    group: 2
    pad: 1
    weight_filler: { type: "bilinear" } 
    bias_term: false
  }
  param { 
  	lr_mult: 0.1 
  	decay_mult: 1 
  }
}

layer {
  name: "score-4f"
  type: "Convolution"
  bottom: "res4f"
  top: "score-4f"
  param {
    lr_mult: 0.1
    decay_mult: 1
  }
  param {
    lr_mult: 0.2
    decay_mult: 0
  }
  convolution_param {
    num_output: 2
    kernel_size: 1
    engine: CUDNN
  }
}

layer {
	name: "fusion-4"
	bottom: "score-4f"
	bottom: "upscore-5c"
	top: "fusion-4"
	type:"Eltwise"
}

######## 16

layer {
  name: "upscore-fusion-4"
  type: "Deconvolution"
  bottom: "fusion-4"
  top: "upscore-fusion-4"
  convolution_param {
    kernel_size: 4
    stride: 2
    num_output: 2
    group: 2
    pad: 1
    weight_filler: { type: "bilinear" } 
    bias_term: false
  }
  param { 
  	lr_mult: 0.1
  	decay_mult: 1
  }
}

layer {
  name: "score-3d"
